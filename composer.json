{
    "name": "andreslqr/laravel-env-cli",
    "description": "This package allows to update the .env file from your laravel project via terminal",
    "keywords": [
        "laravel",
        "env",
        "command",
        "terminal"
    ],
    "autoload": {
        "psr-4": {
            "Andreslqr\\EnvCli\\": "src/"
        }
    },
    "authors": [{
        "name": "Andreslqr",
        "email": "andres22.dev@gmail.com"
    }],
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0"
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andreslqr\\EnvCli\\EnvCliServiceProvider"
            ],
            "aliases": []
        }
    }

}
